{
  "name": "ltlblox",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng:serve": "ng serve --port 4200",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --e2e --browser chrome",
    "e2e": "start-server-and-test ng:serve http://localhost:4200 cypress:run --config pageLoadTimeout=60000",
    "e2e:open": "start-server-and-test ng:serve http://localhost:4200 cypress:open"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@types/three": "^0.178.0",
    "ngx-three": "^0.40.1",
    "rxjs": "~7.8.0",
    "three": "^0.178.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.14",
    "@angular/cli": "^20.3.14",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/platform-browser-dynamic": "^21.1.0",
    "@types/jasmine": "~5.1.0",
    "cypress": "^15.9.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.2",
    "zone.js": "^0.15.1"
  }
}
